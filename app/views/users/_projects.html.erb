<h3 class="c-title -normal -grid wrapper"><%= t "your_projects" %></h3>
<!-- c-grid-projects -->
<div class="c-card-grid" id="content">
  <!-- Provisional loop  -->
  <% for i in 1..4 %>
    <div class="card -mobile-large-1-2 -tablet-1-3">
      <h4 class="c-card-title -border-1"><%= t 'active' %></h4>
      <a>
        <h3 class="c-title">Research Training in Cancer Prevention and Control in Morocco</h3>
      </a>
      <div class="c-tag-list -closer">
        <% ['Breast', 'Bladder', 'Pancreas'].each do |t| %>
        <p class="tag"><%= t %></p>
        <% end %>
      </div>
      <%= link_to (t 'edit'), '/', {:class => "c-button -color-3 -network"} %>
    </div>
  <% end %>

</div>

