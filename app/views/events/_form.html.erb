<%= simple_form_for @event do |e| %>
  <%= e.input :title %>
  <%= e.input :description %>
  <%= e.input :website %>
  <%= e.input :excerpt %>
  <%= e.input :participants %>
  <%= e.input :start_date %>
  <%= e.input :end_date %>
  <%= e.input :private %>
  <%= e.input :online %>
  <%= e.input :address %>
  <%= e.input :address2 %>
  <%= e.input :city %>
  <%= e.input :country, as: :select, collection: Country.names %>
  <%= e.input :state %>
  <%= e.input :postcode %>
  <%= e.input :latitute %>
  <%= e.input :longitude %>
  <%= e.button :submit %>
<% end %>
